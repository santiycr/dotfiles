{
  "default_module": "powerline.segments.common",
  "segment_data": {
    "uptime": {
      "before": "⇑  "
    },
    "external_ip": {
      "before": "ⓦ  "
    },
    "date": {
      "before": ""
    }
  },
  "segments": {
    "left": [
      {
        "name": "uptime",
        "priority": 50
      },
      {
        "name": "system_load",
        "priority": 50
      },
      {
        "name": "battery",
        "args": {
          "steps": 20
        }
      }
    ],
    "right": [
      {
        "name": "external_ip",
        "priority": 50
      },
      {
        "name": "date"
      },
      {
        "name": "date",
        "args": {
          "format": "%H:%M",
          "istime": true
        }
      },
      {
        "name": "hostname"
      }
    ]
  }
}
